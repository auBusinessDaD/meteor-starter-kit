{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2020",
    "jsx": "react",
    "strictFunctionTypes": true,
    "checkJs": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "/*": [
        "./*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    ".meteor"
  ],
  "typeAcquisition": {
    "include": [
      "meteor"
    ]
  }
}